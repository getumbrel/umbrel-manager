    "docker-compose": "^0.23.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "iocane": "^4.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-rsa": "^1.1.1",
